{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ServiceConfiguration": {
    "ServiceDnsName": "crfrontend.yourdomain.com",
    "ServiceCNAME": "your-cname",
    "DefaultCertificate": "ABC0000000000000000000000000000000000CBA",
    "APPINSIGHTS_INSTRUMENTATIONKEY": "",
    "AadAppId": "your-app-id",
    "AadAppSecret": "your-app-secret",
    "PlaceCallEndpointUrl": "",
    "CallControlListeningUrls": [
      "https://0.0.0.0:9441",
      "https://0.0.0.0:10100-10199",
      "https://0.0.0.0:20100-20199"
    ]
  },
  "Endpoints": {
    "DefaultEndpoint": {
      "Protocol": "tcp",
      "Port": 443,
      "LocalPort": 9441
    },
    "InstanceCallControlEndpoint": {
      "Protocol": "tcp",
      "LocalPort": 10100,
      "PublicPortRange": {
        "Min": 10100,
        "Max": 10199,
        "
      }
    },
    "InstanceMediaControlEndpoint": {
      "Protocol": "tcp",
      "LocalPort": 8445,
      "PublicPortRange": {
        "Min": 20100,
        "Max": 20199,
        "
      }
    }
  },
  "Certificates": {
    "Default": {
      "Thumbprint": "ABC0000000000000000000000000000000000CBA",
      "Algorithm": "sha1"
    }
  }
}
